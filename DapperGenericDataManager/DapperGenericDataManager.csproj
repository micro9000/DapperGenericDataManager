<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Raniel Garcia</Authors>
    <Company>RichCorner</Company>
    <Description>Simple Generic Data manager with Basic CRUD methods using Dapper and Dapper.Contrib.Extensions</Description>
